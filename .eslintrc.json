{
    "extends": "airbnb-base",
    "rules": {
        "comma-dangle": ["error", "never"],
        "func-names": ["error", "as-needed"],
        "function-paren-newline": ["error", "consistent"],
        "max-len": ["error", { "code": 80 }],
        "no-plusplus": "off",
        "no-underscore-dangle": ["error", { "allowAfterThis": true }]
    }
}
